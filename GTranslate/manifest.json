{
  "name": "GTranslate",
  "version": "1.1.1.8",
  "manifest_version": 2,
  "description": "RightClick google translate",
  "homepage_url": "http://ahmadalli.net",
  "permissions": ["contextMenus","webRequest", "webRequestBlocking", "http://*/*","notifications"],
  "web_accessible_resources": ["icon.png"],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "background": {
	"page": "backgrounds.html"
  },
  "options_page": "options.html",
  "icons": {
	"16": "icon.png",
    "128": "icon.png"
  }
  
}